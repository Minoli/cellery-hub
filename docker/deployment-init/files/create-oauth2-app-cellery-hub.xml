<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope" xmlns:xsd="http://org.apache.axis2/xsd" xmlns:xsd1="http://dto.oauth.identity.carbon.wso2.org/xsd">
    <soap:Header/>
    <soap:Body>
        <xsd:registerOAuthApplicationData>
            <!--Optional:-->
            <xsd:application>
                <xsd1:OAuthVersion>OAuth-2.0</xsd1:OAuthVersion>
                <xsd1:applicationAccessTokenExpiryTime>3600</xsd1:applicationAccessTokenExpiryTime>
                <xsd1:applicationName>CelleryWebPortal</xsd1:applicationName>
                <xsd1:backChannelLogoutUrl/>
                <xsd1:bypassClientCredentials>false</xsd1:bypassClientCredentials>
                <xsd1:callbackUrl>{CELLERY_HUB_WEB_PORTAL_CALLBACK}</xsd1:callbackUrl>
                <xsd1:grantTypes>password authorization_code</xsd1:grantTypes>
                <xsd1:idTokenEncryptionEnabled>false</xsd1:idTokenEncryptionEnabled>
                <xsd1:idTokenExpiryTime>3600</xsd1:idTokenExpiryTime>
                <xsd1:oauthConsumerKey>celleryhubapplication</xsd1:oauthConsumerKey>
                <xsd1:oauthConsumerSecret>celleryhubapplication</xsd1:oauthConsumerSecret>
                <xsd1:pkceMandatory>false</xsd1:pkceMandatory>
                <xsd1:pkceSupportPlain>true</xsd1:pkceSupportPlain>
                <xsd1:refreshTokenExpiryTime>84600</xsd1:refreshTokenExpiryTime>
                <xsd1:requestObjectSignatureValidationEnabled>false</xsd1:requestObjectSignatureValidationEnabled>
                <xsd1:tokenType>Default</xsd1:tokenType>
                <xsd1:userAccessTokenExpiryTime>604800</xsd1:userAccessTokenExpiryTime>
            </xsd:application>
        </xsd:registerOAuthApplicationData>
    </soap:Body>
</soap:Envelope>
